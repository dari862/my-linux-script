if [ "$1" == "-f" ]; then
	x-terminal-emulator
elif [ "$1" == "-s" ]; then
	x-terminal-emulator
elif [ "$1" == "-e" ]; then
	x-terminal-emulator -e "$2"
else
	x-terminal-emulator
fi
